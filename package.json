{
  "name": "postit-webapp",
  "version": "0.0.1",
  "description": "server side implementation of postit",
  "main": "app.js",
  "scripts": {
    "exec": "nodemon ./server/bin/www --exec babel-node .server/app",
    "babel-version": "babel --version",
    "build": "babel src -d lib",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "start": "nodemon ./server/bin/www --exec babel-node .server/app",
    "test_": "nyc mocha --compilers js:babel-core/register --timeout 10000",
    "test-dev": "mocha --compilers js:babel-core/register --timeout 10000",
    "test": "nyc --reporter=html --reporter=text mocha --compilers js:babel-register",
    "start-client": "cd client && npm run start",
    "build-client": "cd client && npm run build",
    "test-client": "cd client && npm run test",
    "start-full": "concurrently \"npm run start\" \"npm run client\"",
    "start-prod": "concurrently \"npm run start\" \"npm run client-prod\"",
    "client": "node start-client.js",
    "client-prod": "node start-client-prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dinobi/Bootcamp-LOS-24-POSTIT.git"
  },
  "keywords": [
    "server",
    "node",
    "postit"
  ],
  "author": "Dinobi Kenkwo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dinobi/Bootcamp-LOS-24-POSTIT/issues"
  },
  "homepage": "https://github.com/dinobi/Bootcamp-LOS-24-POSTIT#readme",
